{
    "name": "@pinnacle0/webpack-util",
    "version": "0.8.0-beta.3",
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": "./src/index.ts"
    },
    "scripts": {
        "run-script": "tsx",
        "build": "pnpm run run-script script/build.ts",
        "format": "pnpm run run-script script/format.ts",
        "test": "vitest --config config/vitest.config.ts --run",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "@rspack/cli": "1.1.5",
        "@rspack/core": "1.1.5",
        "@rspack/dev-server": "1.0.9",
        "@rspack/plugin-react-refresh": "1.0.0",
        "@swc/core": "1.4.4",
        "css-loader": "7.1.2",
        "ts-checker-rspack-plugin": "1.0.3",
        "html-webpack-plugin": "5.6.0",
        "less": "4.2.0",
        "less-loader": "12.2.0",
        "pretty-format": "29.7.0",
        "react-refresh": "0.14.2",
        "socks-proxy-agent": "8.0.3",
        "style-loader": "4.0.0",
        "swc-plugin-core-fe-hmr": "1.0.5",
        "yargs": "17.7.2",
        "@biomejs/biome": "1.9.4",
        "@biomejs/js-api": "0.7.1",
        "@biomejs/wasm-nodejs": "1.9.4"
    },
    "devDependencies": {
        "@pinnacle0/devtool-util": "workspace:*",
        "@types/express": "4.17.21",
        "@types/html-webpack-plugin": "3.2.9",
        "@types/yargs": "17.0.32"
    },
    "engines": {
        "node": ">=20"
    },
    "publishConfig": {
        "directory": "build",
        "linkDirectory": false,
        "exports": {
            ".": {
                "import": "./src/index.js",
                "types": "./src/index.d.ts"
            }
        }
    }
}
