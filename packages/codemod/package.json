{
    "name": "@pinnacle0/codemod",
    "version": "1.0.0",
    "description": "Provide codemod by using JSCodeShift",
    "main": "build/index.js",
    "bin": {
        "pinnacle-codemod": "./build/bin.js"
    },
    "files": [
        "build"
    ],
    "exports": {
        ".": "./build/index.js"
    },
    "scripts": {
        "run-script": "ts-node --project ./script/tsconfig.json ",
