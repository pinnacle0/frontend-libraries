    "dependencies": {
        "@babel/parser": "7.19.3",
        "ast-types": "0.14.2",
        "fs-extra": "10.1.0",
        "glob": "8.0.3",
        "recast": "0.21.5",
        "yargs": "17.6.0"
    },
    "devDependencies": {
        "@pinnacle0/devtool-util": "workspace:*",
        "@types/glob": "8.0.0"
    }
